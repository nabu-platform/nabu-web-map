<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<crud>
    <allowHeaderAsQueryParameter>true</allowHeaderAsQueryParameter>
    <broadcastCreate>false</broadcastCreate>
    <broadcastUpdate>false</broadcastUpdate>
    <coreType>nabu.web.map.database.types.sitemapApplication</coreType>
    <createPermission>application.edit</createPermission>
    <createRole>$user</createRole>
    <deletePermission>application.edit</deletePermission>
    <deleteRole>$user</deleteRole>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>name</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>webApplicationId</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>name</key>
        <operator>ilike</operator>
        <or>false</or>
        <alias>q</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>webApplicationId</key>
        <operator>ilike</operator>
        <or>true</or>
        <alias>q</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <getPermission>application.edit</getPermission>
    <hooks>false</hooks>
    <listPermission>application.edit</listPermission>
    <listRole>$user</listRole>
    <provider>nabu.services.crud.provider.basic.provider</provider>
    <restLimitToUser>true</restLimitToUser>
    <updatePermission>application.edit</updatePermission>
    <updateRole>$user</updateRole>
    <useAsAuthorizationServiceContext>false</useAsAuthorizationServiceContext>
    <useExplicitLanguage>false</useExplicitLanguage>
    <useGlobalPermissionContext>false</useGlobalPermissionContext>
    <useLanguage>false</useLanguage>
    <useListOutputForCreate>false</useListOutputForCreate>
    <useListOutputForUpdate>false</useListOutputForUpdate>
    <useProjectAsPermissionContext>false</useProjectAsPermissionContext>
    <useServiceContextAsPermissionContext>false</useServiceContextAsPermissionContext>
    <useWebApplicationAsPermissionContext>true</useWebApplicationAsPermissionContext>
</crud>
